<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.starbucks.dao.MapDao">

    <select id="findAll" resultType="com.example.starbucks.dto.StoreDto">
        select * from starbugs_store
    </select>
    <insert id="insertStoreList" parameterType="com.example.starbucks.dto.StoreDto">
        INSERT INTO starbugs_store (
            store_name, x, y, store_type, store_addr
        ) VALUES (
            #{storeName}, #{x}, #{y}, #{storeType}, #{storeAddr}
        )
    </insert>

</mapper>